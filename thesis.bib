	Eprint = {0709.3584},
	Journal = {ArXiv e-prints},
	Keywords = {physics, classical physics, physics, general physics, dimensional analysis, data mining},
	Month = sep,
	Rating = {5},
	Title = {{100 Years of dimensional analysis: New steps toward empirical law deduction}},
	Volume = 709,
	Year = 2007,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/abs/2007arXiv0709.3584T},
