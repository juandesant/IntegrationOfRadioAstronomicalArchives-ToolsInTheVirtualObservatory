	Eprint = {0806.1670},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics},
	Month = jun,
	Title = {{The Arecibo Legacy Fast ALFA (ALFALFA) Extragalactic HI Survey}},
	Volume = 806,
	Year = 2008,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/abs/2008arXiv0806.1670H},
